{
  "plugins": [
    [
      "babel-plugin-add-header-comment",
      {
        "commentStart": "*********\n",
        "commentLineStart": "\t",
        "commentEnd": "\n*********",
        "header": [ "RELEASE" ]
      }
    ]
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": { 
          "node": "current" 
        } 
      }
    ]
  ]
}
