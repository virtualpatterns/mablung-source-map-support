{
  "author": "virtualpatterns.com <code@virtualpatterns.com> (http://www.virtualpatterns.com)",
  "ava": {
    "failFast": true,
    "files": [
      "release/commonjs/test/**/*.test.*",
      "release/esmodule/test/**/*.test.*"
    ],
    "nodeArguments": [
      "--no-warnings",
      "--unhandled-rejections=strict"
    ],
    "require": [
      "./release/esmodule/install.js"
    ],
    "verbose": true
  },
  "c8": {
    "all": true,
    "exclude": [
      "release/commonjs/test",
      "release/esmodule/test",
      "source/commonjs/test",
      "source/esmodule/test"
    ],
    "reporter": [
      "html"
    ]
  },
  "description": "TBD",
  "exports": {
    ".": {
      "import": "./release/esmodule/index.js",
      "require": "./release/commonjs/index.cjs"
    },
    "./install": {
      "import": "./release/esmodule/install.js",
      "require": "./release/commonjs/install.cjs"
    }
  },
  "license": "GPL-3.0+",
  "main": "release/commonjs/index.cjs",
  "name": "@virtualpatterns/mablung-source-map-support",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/mablung-source-map-support"
  },
  "scripts": {
    "test:10": "make test",
    "test": "run-s --silent \"test:10 argument='{@}'\" --",
    "node:10": "make node",
    "node": "run-s --silent \"node:10 argument='{@}'\" --"
  },
  "type": "module",
  "version": "0.0.2-25",
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/eslint-parser": "^7.13.14",
    "@babel/preset-env": "^7.14.1",
    "ava": "^3.15.0",
    "c8": "^7.7.2",
    "depcheck": "^1.4.1",
    "eslint": "^7.26.0",
    "eslint-plugin-babel": "^5.3.1",
    "npm-check-updates": "^11.5.11",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3"
  },
  "dependencies": {
    "source-map-support": "^0.5.19"
  }
}
